!begin_node Bindings for Fseek
!ignore_index
(!begin_liste) [Assembler:]
!item [C:]
int32_t Fseek ( int32_t offset, int16_t handle, int16_t seekmode );
!item [Assembler:]
!begin_verbatim
move.w    seekmode,-(sp)  ; Offset 8
move.w    handle,-(sp)    ; Offset 6
move.l    offset,-(sp)    ; Offset 2
