                               RECT16 *dst_clip_rect,
                               RECT16 *src_rect,
                               RECT16 *dst_rect,
                               RECT16 *clipped_src_rect,
                               RECT16 *clipped_dst_rect )
{
   ptsin[0..3] = dst_clip_rect; 
   ptsin[4..7] = src_rect; 
   ptsin[8..11] = dst_rect; 

   contrl[0] = 171;
   contrl[1] = 6;
