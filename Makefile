#
all: $(STGPATH)/linea.stg $(STGPATH)/xbios.stg

#
$(STGPATH)/linea.stg: e_linea.u $(LINEAFILES)
	bash -c "udo $(STGFLAGS) -o $(STGPATH)/linea.stg e_linea.u; exit 0"

#
$(STGPATH)/xbios.stg: e_xbios.u $(XBIOSFILES)
	bash -c "udo $(STGFLAGS) -o $(STGPATH)/xbios.stg e_xbios.u; exit 0"

#
